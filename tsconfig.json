{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",

        "esModuleInterop": true,
        "alwaysStrict": true,
        "downlevelIteration": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noFallthroughCasesInSwitch": true,
        "allowSyntheticDefaultImports": true,

        "removeComments": true,


        "lib": [
            "es2015"
        ],
        "outDir": "./target"
    }
}